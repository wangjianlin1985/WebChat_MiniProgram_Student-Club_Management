<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.model.User" table="users">
		<comment>用户表</comment>
		<id name="id" type="java.lang.Integer">
			<column name="id" length="11">
				<comment>主键</comment>
			</column>
			<generator class="native"></generator>
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" length="20">
				<comment>姓名</comment>
			</column>
		</property>
		<property name="code" type="java.lang.String">
			<column name="code" length="20">
				<comment>账号</comment>
			</column>
		</property>
		<property name="password" type="java.lang.String">
			<column name="password" length="20">
				<comment>密码</comment>
			</column>
		</property>
		<property name="role" type="java.lang.String">
			<column name="role" length="20">
				<comment>角色</comment>
			</column>
		</property>
		<property name="idcard" type="java.lang.String">
			<column name="idcard" length="18">
				<comment>身份证</comment>
			</column>
		</property>
		<property name="phone" type="java.lang.String">
			<column name="phone" length="11">
				<comment>手机号</comment>
			</column>
		</property>
		<property name="email" type="java.lang.String">
			<column name="email" length="20">
				<comment>邮箱</comment>
			</column>
		</property>
		<property name="address" type="java.lang.String">
			<column name="address" length="128">
				<comment>地址</comment>
			</column>
		</property>
		<property name="notes" type="java.lang.String">
			<column name="notes" length="64">
				<comment>备注</comment>
			</column>
		</property>
		<property name="createdAt" type="java.lang.String">
			<column name="createdAt" length="20">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="updatedAt" type="java.lang.String">
			<column name="updatedAt" length="20">
				<comment>更新时间</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
