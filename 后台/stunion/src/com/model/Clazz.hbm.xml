<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.model.Clazz" table="clazz">
		<comment>班级信息表</comment>
		<id name="id" type="java.lang.Integer">
			<column name="id" length="11">
				<comment>主键</comment>
			</column>
			<generator class="native"></generator>
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" length="20">
				<comment>名称</comment>
			</column>
		</property>
		<property name="notes" type="java.lang.String">
			<column name="notes" length="256">
				<comment>备注</comment>
			</column>
		</property>
		<property name="createdAt" type="java.lang.String">
			<column name="createdAt" length="20">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="updatedAt" type="java.lang.String">
			<column name="updatedAt" length="20">
				<comment>更新时间</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
